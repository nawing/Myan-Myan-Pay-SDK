{
  "name": "myanmyan-pay-sdk",
  "version": "1.0.0",
  "description": "JavaScript SDK for integrating the MMQR Merchant and Redirect payment gateway",
  "type": "module",
  "keywords": [
    "MMQR",
    "Master",
    "Visa",
    "JCB",
    "MPU"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.10.6",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "author": "Naw Ing",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0"
  }
}
